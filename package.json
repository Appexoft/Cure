    "test:coverage": "jest --coverage",
    "run:ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "run:ios:clean": "react-native run-ios --reset-cache",
    "run:android": "react-native run-android",
    "run:android:clean": "react-native run-android --reset-cache",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean:android": "cd android && ./gradlew clean && cd ../",
